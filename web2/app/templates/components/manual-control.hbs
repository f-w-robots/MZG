{{#each devices as |deviceId|}}
  <div class="col-md-1 col-md-offset-1">
    <a href="#" {{action "select" deviceId}}>
      <img src="images/device.png" height="100">
      {{deviceId}}
    </a>
  </div>
{{/each}}
{{#if url}}
  <iframe src="{{url}}" width="600" height="400" class="control">
  </iframe>
{{/if}}
