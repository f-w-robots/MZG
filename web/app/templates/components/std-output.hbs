<div class="std-output output" style="border: 4px double black; height: 200px; overflow: scroll">
    <i>output:</i>
  <br>
  {{#each data as |line|}}
    {{#if line.stderr}}
    <div class="line stderr">
      {{line.line}}
    </div>
    {{/if}}
    {{#if line.stdout}}
    <div class="line stdout">
      {{line.line}}
    </div>
    {{/if}}
  {{/each}}
</div>
